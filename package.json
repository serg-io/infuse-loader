{
  "name": "infuse-loader",
  "version": "0.0.1",
  "description": "Webpack loader for parsing and importing infuse.host templates.",
  "license": "MIT",
  "author": "Sergio Alcantara <iserg9@gmail.com> (https://github.com/serg-io)",
  "homepage": "https://github.com/serg-io/infuse-loader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serg-io/infuse-loader.git"
  },
  "keywords": [
    "infuse",
    "HTML",
    "template",
    "engine",
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/serg-io/infuse-loader/issues"
  },
  "main": "lib/loader.js",
  "files": [
    ".babelrc",
    "/lib",
    "/src"
  ],
  "scripts": {
    "lint": "eslint src/*.js",
    "postversion": "git push && git push --tags && npm publish",
    "prepare": "rm -rf lib && npm run transpile",
    "preversion": "npm run lint && npm run test",
    "test": "jest",
    "transpile": "babel src --out-dir lib --ignore *.test.js,__fixtures__"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "infuse.host": "0.0.1",
    "loader-utils": "^1.1.0"
  },
  "peerDependencies": {
    "infuse.host": "0.0.1",
    "webpack": "^4.20.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "memory-fs": "^0.4.1",
    "merge": "^1.2.1",
    "webpack": "^4.24.0"
  }
}